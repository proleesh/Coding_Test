<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd" default-lazy-init="true">
	 
	<bean id="shaDigest" class="com.example.naengmyeon.factory.MessageDigestFactoryBean" p:algorithmName="SHA1"/>
	<bean id="defaultDigest" class="com.example.naengmyeon.factory.MessageDigestFactoryBean"></bean>
	<bean id="digester" class="com.example.naengmyeon.factory.MessageDigester"
	p:digest1-ref="shaDigest" p:digest2-ref="defaultDigest"/>
	</beans>